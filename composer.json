{
    "name": "vtec/crud",
    "description": "",
    "keywords": [
        "vtec",
        "vtec-crud"
    ],
    "homepage": "https://github.com/adr1enbe4udou1n/vtec-crud",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Beaudouin",
            "email": "adrien.beaudouin@outlook.com",
            "homepage": "https://okami101.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/routing": "^7.5",
        "illuminate/support": "^7.5",
        "illuminate/validation": "^7.5",
        "spatie/laravel-medialibrary": "^8.1",
        "spatie/laravel-query-builder": "^2.8",
        "spatie/laravel-translatable": "^4.3",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vtec\\Crud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vtec\\Crud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vtec\\Crud\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
