{
    "name": "vtec/laravel-crud",
    "description": "Laravel CRUD generator based on Vtec Admin, a SPA admin panel based on Vuetify.",
    "keywords": [
        "laravel",
        "crud",
        "api",
        "admin",
        "vue",
        "vuetify",
        "vue-admin",
        "vuetify-admin",
        "vtec",
        "vtec-admin"
    ],
    "homepage": "https://vtec.okami101.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Beaudouin",
            "email": "adrien.beaudouin@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/validation": "^7.0",
        "spatie/laravel-medialibrary": "^7.0",
        "spatie/laravel-query-builder": "^3.0",
        "spatie/laravel-translatable": "^4.3",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vtec\\Crud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vtec\\Crud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vtec\\Crud\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
