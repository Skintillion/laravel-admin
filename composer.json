{
    "name": "okami101/laravel-vuetify-admin",
    "description": "Laravel admin panel generator using Vuetify Admin, a separate SPA admin framework running on top of REST APIs and built on Vuetify.",
    "keywords": [
        "laravel",
        "crud",
        "api",
        "admin",
        "vue",
        "vuetify",
        "vue-admin",
        "vuetify-admin"
    ],
    "homepage": "https://www.okami101.io/vuetify-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Beaudouin",
            "email": "adrien.beaudouin@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/validation": "^7.0",
        "spatie/laravel-medialibrary": "^7.0",
        "spatie/laravel-query-builder": "^3.0",
        "spatie/laravel-translatable": "^4.3",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Okami101\\LaravelVuetifyAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okami101\\LaravelVuetifyAdmin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okami101\\LaravelVuetifyAdmin\\AdminServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
